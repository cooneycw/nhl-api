# NHL API Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to git (it's in .gitignore)

# AWS Credentials for Secrets Manager
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# AWS Secrets Manager Secret ID for PostgreSQL credentials
# This secret should contain: host, port, database, username, password
NHL_DB_SECRET_ID=nhl-api
